enum FavFood {
  banana
  apple
}

model Profile {
  id      String @id
  user    User   @relation(fields: [userId1, userId2], references: [id, id2])
  userId1 String
  userId2 String
}

model User {
  id      String
  id2     String
  x       Int
  y       Int
  s       String   @db.Bit
  profile Profile?

  @@unique([id, id2])
  @@unique([x, y], name: "algebra")
}