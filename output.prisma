datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum FavFood {
  b
  a
}

model Profile {
  id      String @id
  user    UsEr   @relation(fields: [userId, userId2], references: [id, id2])
  userId  String
  userId2 Int
}

model UsEr {
  id      String
  id2     Int
  x       Int
  y       Int
  s       String   @db.Text
  profile Profile?

  @@id([id, id2])
  @@unique([x, y], name: "algebra")
  @@unique([s, y], name: "SomeName")
}
