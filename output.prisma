datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum FavFood {
  b
  a
}

model Profile {
  id     String @id
  user   UsEr   @relation(fields: [userId], references: [id])
  userId String
  x      UsEr   @relation("x", fields: [usErId], references: [id])
  usErId String
}

model UsEr {
  id      String   @id
  id2     Int
  x       Int
  y       Int
  s       String[]
  profile Profile?

  Profile Profile[] @relation("x")
  @@unique([x, y], name: "algebra")
  @@unique([s, y], name: "SomeName")
}
