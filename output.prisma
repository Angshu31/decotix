datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum FavFood {
  banana
  apple
}

model Profile {
  id      String       @id
  user    TheUserModel @relation(fields: [userid, userid2], references: [id, id2])
  userid  String
  userid2 Int
}

model TheUserModel {
  id      String
  id2     Int
  x       Int
  y       Int
  s       String   @db.Text
  profile Profile?

  @@unique([id, id2])
  @@unique([x, y], name: "algebra")
}